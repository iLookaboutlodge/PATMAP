    <!-- To Fix IE11 Report Print Button Not showing -->
    <rewrite>
      <rules>
        <rule name="Change IE11 User Agent to IE9" enabled="true">
          <match url="(.*)" />
          <action type="None" />
          <conditions>
            <add input="{HTTP_USER_AGENT}" pattern=".trident\/[789].*Gecko" />
          </conditions>
          <serverVariables>
            <set name="HTTP_USER_AGENT" value="Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.0; Trident/5.0)" />
          </serverVariables>
        </rule>
      </rules>
    </rewrite>
